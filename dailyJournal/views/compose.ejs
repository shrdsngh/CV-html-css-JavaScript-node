<%- include("partials/header") -%>
   <h1>Compose</h1>

   <form class="" action="/compose" method="post">
     <div class="form-group">
       <label>Title</label>
       <input class="form-control" name="postTitle" type="text" autocomplete="off">
       <br>
       <label>Post</label>
       <textarea class="form-control" name="postBody" rows="8" cols="80" autocomplete="off"></textarea>
     </div>
     <button type="submit" class="btn btn-primary" name="button">Publish</button>
   </form>

<%- include("partials/footer") -%>
